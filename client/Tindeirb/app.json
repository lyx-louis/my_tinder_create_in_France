{
  "expo": {
    "name": "Tindeirb",
    "slug": "Tindeirb",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
        "infoPlist": {
          "UIBackgroundModes": [
            "location",
            "fetch"
          ],
          "NSCameraUsageDescription": "azerty",
          "NSPhotoLibraryUsageDescription": "azerty",
          "NSMicrophoneUsageDescription": "azerty",
      }
    },
    "android": {
      "permissions": ["ACCESS_FINE_LOCATION","ACCESS_COARSE_LOCATION","ACCESS_BACKGROUND_LOCATION","CAMERA","READ_EXTERNAL_STORAGE","WRITE_EXTERNAL_STORAGE"],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
